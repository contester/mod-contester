<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/contester/db" VERSION="20101203" COMMENT="XMLDB file for Moodle mod/contester"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="contester" COMMENT="Top level contester table to associate contests with courses">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="Course contester activity belongs to"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="name field for moodle instances"/>
	<FIELD NAME="intro" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="General introduction of the contester activity"/>
	<FIELD NAME="introformat" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="Format of the intro field (MOODLE, HTML, MARKDOWN...)"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>		
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="description" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="freeview" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="viewown" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" DEFAULT="1" SEQUENCE="false"/>
	 <FIELD NAME="iomethodmode" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="course" UNIQUE="false" FIELDS="course"/>
      </INDEXES>
    </TABLE>

    <TABLE NAME="contester_languages" COMMENT="Programming languages available to use">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="name"/>        
        <FIELD NAME="name" TYPE="char" LENGTH="64" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="ext"/>
	  <FIELD NAME="ext" TYPE="char" LENGTH="16" NOTNULL="true" SEQUENCE="false" PREVIOUS="name"/>
        <FIELD NAME="display" TYPE="int" LENGTH="10"/>        
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="contester_language_map" COMMENT="Defines contester">
      <FIELDS>
        <FIELD NAME="contester_id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" NEXT="language_id"/>        
        <FIELD NAME="language_id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="contester_id"/>                
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="contester_id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="contester_problemmap" COMMENT="Defines contester">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="problemid"/>        
        <FIELD NAME="problemid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="id" NEXT="contesterid"/>                
        <FIELD NAME="contesterid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="problemid"/>                
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="contester_problems" COMMENT="Defines contester">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="name"/>        
        <FIELD NAME="name" TYPE="char" LENGTH="64" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="dbid"/>
        <FIELD NAME="dbid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" NEXT="description"/>
        <FIELD NAME="description" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" PREVIOUS="dbid" NEXT="input_format"/>
	  <FIELD NAME="input_format" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" PREVIOUS="dbid" NEXT="output_format"/>
        <FIELD NAME="output_format" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" PREVIOUS="input_format"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="contester_resultdesc" COMMENT="Result descriptions in different languages">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" NEXT="success"/>        
        <FIELD NAME="success" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="id" NEXT="description"/>        
        <FIELD NAME="description" TYPE="char" LENGTH="64" NOTNULL="true" SEQUENCE="false" PREVIOUS="success" NEXT="language"/>
        <FIELD NAME="language" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" DEFAULT="1" SEQUENCE="false"  PREVIOUS="description"/>        
	<FIELD NAME="uid" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="true"/>      
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="uid"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="contester_results" COMMENT="Individual test results">
      <FIELDS>
        <FIELD NAME="testingid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" NEXT="processed_uts"/>
	 <FIELD NAME="processed_uts" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="testingid" NEXT="result"/>
        <FIELD NAME="result" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="processed_uts" NEXT="test"/>        
        <FIELD NAME="test" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="result" NEXT="timex"/>       
         <FIELD NAME="timex" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" PREVIOUS="test" NEXT="memory"/>
         <FIELD NAME="memory" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" PREVIOUS="timex" NEXT="info"/>
        <FIELD NAME="info" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" PREVIOUS="memory" NEXT="testeroutput"/>
        <FIELD NAME="testeroutput" TYPE="text" LENGTH="big" NOTNULL="false"  PREVIOUS="success" NEXT="testererror"/>
        <FIELD NAME="testererror" TYPE="text" LENGTH="big" NOTNULL="false"  PREVIOUS="testeroutput" NEXT="testerexitcode"/>
        <FIELD NAME="testerexitcode" TYPE="int" LENGTH="10" UNSIGNED="true" NOTNULL="false" DEFAULT="0" PREVIOUS="testererror"/>        
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="testingid"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="contester_samples" COMMENT="Defines contester">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" NEXT="problem_id"/>
        <FIELD NAME="problem_id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="id" NEXT="number"/>        
        <FIELD NAME="number" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="problem_id" NEXT="input"/>
        <FIELD NAME="input" TYPE="text" LENGTH="big" NOTNULL="false" DEFAULT="NULL" PREVIOUS="number" NEXT="output"/>
        <FIELD NAME="output" TYPE="text" LENGTH="big" NOTNULL="false" DEFAULT="NULL" PREVIOUS="input"/>        
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="contester_submits" COMMENT="Defines contester">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="contester"/>
        <FIELD NAME="contester" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="id" NEXT="student"/>
        <FIELD NAME="student" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="contester" NEXT="problem"/>
        <FIELD NAME="problem" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="student" NEXT="lang"/>
        <FIELD NAME="lang" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="problem" NEXT="iomethod"/>
	 <FIELD NAME="iomethod" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" PREVIOUS="lang" NEXT="solution"/>
        <FIELD NAME="solution" TYPE="binary" NOTNULL="false" PREVIOUS="iomethod" NEXT="submitted"/>
        <FIELD NAME="submitted" TYPE="datetime" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="solution" NEXT="submitted_uts"/>
        <FIELD NAME="submitted_uts" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="submitted" NEXT="processed"/>
        <FIELD NAME="processed" TYPE="int" LENGTH="10" UNSIGNED="true" NOTNULL="false" PREVIOUS="submitted_uts"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="contester_tagmap" COMMENT="Defines contester">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="problemid"/>
        <FIELD NAME="problemid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" PREVIOUS="id" NEXT="tagid"/>        
        <FIELD NAME="tagid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" PREVIOUS="problemid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="contester_tags" COMMENT="Defines contester">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="tag"/>        
        <FIELD NAME="tag" TYPE="char" LENGTH="64" NOTNULL="true" PREVIOUS="id"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="contester_testings" COMMENT="Defines contester">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="submitid"/>
        <FIELD NAME="submitid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="id" NEXT="start"/>
        <FIELD NAME="start" TYPE="datetime" NOTNULL="true" DEFAULT="CURRENT_TIMESTAMP" PREVIOUS="submitid" NEXT="start_uts"/>
        <FIELD NAME="start_uts" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="start" NEXT="finish"/>
        <FIELD NAME="finish" TYPE="datetime" NOTNULL="false" DEFAULT="NULL" PREVIOUS="start_uts" NEXT="finish_uts"/> -->
        <FIELD NAME="finish_uts" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" PREVIOUS="finish_uts" NEXT="compiled"/>
        <FIELD NAME="compiled" TYPE="int" LENGTH="1" NOTNULL="false" UNSIGNED="true"  PREVIOUS="finish_uts" NEXT="taken"/>  
        <FIELD NAME="taken" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true"  PREVIOUS="compiled" NEXT="passed"/>
        <FIELD NAME="passed" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" PREVIOUS="taken"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

  </TABLES>
</XMLDB>
