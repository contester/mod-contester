<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/contester/db" VERSION="20101203" COMMENT="XMLDB file for Moodle mod/contester"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="mdl_contester">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="course"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" PREVIOUS="course" NEXT="timecreated"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="name" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="timecreated" NEXT="description"/>
        <FIELD NAME="description" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" PREVIOUS="timemodified" NEXT="freeview"/>
        <FIELD NAME="freeview" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="description" NEXT="viewown"/>
        <FIELD NAME="viewown" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" DEFAULT="1" SEQUENCE="false" PREVIOUS="freeview"/>       
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="course" UNIQUE="false" FIELDS="course"/>
      </INDEXES>
    </TABLE>

    <TABLE NAME="mdl_contester_languages">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="name"/>        
        <FIELD NAME="name" TYPE="char" LENGTH="64" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="ext"/>
	  <FIELD NAME="ext" TYPE="char" LENGTH="16" NOTNULL="true" SEQUENCE="false" PREVIOUS="name"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="mdl_contester_language_map">
      <FIELDS>
        <FIELD NAME="contester_id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" NEXT="language_id"/>        
        <FIELD NAME="language_id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="contester_id"/>                
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="contester_id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="mdl_contester_problemmap">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="problemid"/>        
        <FIELD NAME="problemid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="id" NEXT="contesterid"/>                
        <FIELD NAME="contesterid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="problemid"/>                
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="mdl_contester_problems">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="name"/>        
        <FIELD NAME="name" TYPE="char" LENGTH="64" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="dbid"/>
        <FIELD NAME="dbid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" NEXT="description"/>
        <FIELD NAME="description" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" PREVIOUS="dbid" NEXT="input_format"/>
	  <FIELD NAME="input_format" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" PREVIOUS="dbid" NEXT="output_format"/>
        <FIELD NAME="output_format" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" PREVIOUS="input_format"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="mdl_contester_resultdesc">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" NEXT="success"/>        
        <FIELD NAME="success" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="id" NEXT="description"/>        
        <FIELD NAME="description" TYPE="char" LENGTH="64" NOTNULL="true" SEQUENCE="false" PREVIOUS="success" NEXT="language"/>
        <FIELD NAME="language" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" DEFAULT="1" SEQUENCE="false"  PREVIOUS="description"/>        
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="mdl_contester_results">
      <FIELDS>
        <FIELD NAME="testingid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" NEXT="processed"/>
        <FIELD NAME="processed" TYPE="timestamp" NOTNULL="true" DEFAULT="CURRENT_TIMESTAMP" ONUPDATE="CURRENT_TIMESTAMP" PREVIOUS="testingid" NEXT="result"/>        
        <FIELD NAME="result" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="processed" NEXT="test"/>        
        <FIELD NAME="test" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="result" NEXT="timex"/>       
        <FIELD NAME="timex" TYPE="bigint" LENGTH="20" NOTNULL="false" UNSIGNED="true" DEFAULT="NULL" PREVIOUS="test" NEXT="memory"/>
        <FIELD NAME="memory" TYPE="bigint" LENGTH="20" NOTNULL="false" UNSIGNED="true" DEFAULT="NULL" PREVIOUS="timex" NEXT="info"/>
        <FIELD NAME="info" TYPE="bigint" LENGTH="20" NOTNULL="false" UNSIGNED="true" DEFAULT="NULL" PREVIOUS="memory" NEXT="testeroutput"/>
        <FIELD NAME="testeroutput" TYPE="text" LENGTH="big" NOTNULL="false"  DEFAULT="NULL" PREVIOUS="success" NEXT="testererror"/>
        <FIELD NAME="testererror" TYPE="text" LENGTH="big" NOTNULL="false"  DEFAULT="NULL" PREVIOUS="testeroutput" NEXT="testerexitcode"/>
        <FIELD NAME="testerexitcode" TYPE="int" LENGTH="10" UNSIGNED="true" NOTNULL="false" DEFAULT="NULL" PREVIOUS="testererror"/>        
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="testingid"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="mdl_contester_samples">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" NEXT="problem_id"/>
        <FIELD NAME="problem_id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="id" NEXT="number"/>        
        <FIELD NAME="number" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="problem_id" NEXT="input"/>
        <FIELD NAME="input" TYPE="text" LENGTH="big" NOTNULL="false" DEFAULT="NULL" PREVIOUS="number" NEXT="output"/>
        <FIELD NAME="output" TYPE="text" LENGTH="big" NOTNULL="false" DEFAULT="NULL" PREVIOUS="input"/>        
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="mdl_contester_submits">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="contester"/>
        <FIELD NAME="contester" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="id" NEXT="student"/>
        <FIELD NAME="student" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="contester" NEXT="problem"/>
        <FIELD NAME="problem" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="student" NEXT="lang"/>
        <FIELD NAME="lang" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="problem" NEXT="solution"/>
        <FIELD NAME="solution" TYPE="longblob" NOTNULL="true" PREVIOUS="lang" NEXT="submitted"/>
        <FIELD NAME="submitted" TYPE="timestamp" NOTNULL="true" DEFAULT="CURRENT_TIMESTAMP" PREVIOUS="solution" NEXT="processed"/>
        <FIELD NAME="processed" TYPE="int" LENGTH="10" UNSIGNED="true" NOTNULL="false" PREVIOUS="submitted"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="mdl_contester_tagmap">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="problemid"/>
        <FIELD NAME="problemid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" PREVIOUS="id" NEXT="tagid"/>        
        <FIELD NAME="tagid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" PREVIOUS="problemid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="mdl_contester_tags">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="tag"/>        
        <FIELD NAME="tag" TYPE="char" LENGTH="64" NOTNULL="true" PREVIOUS="id"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="mdl_contester_testings">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="submitid"/>
        <FIELD NAME="submitid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" PREVIOUS="id" NEXT="start"/>
        <FIELD NAME="start" TYPE="timestamp" NOTNULL="true" DEFAULT="CURRENT_TIMESTAMP" PREVIOUS="submitid" NEXT="finish"/>
        <FIELD NAME="finish" TYPE="timestamp" NOTNULL="false" DEFAULT="NULL" PREVIOUS="start" NEXT="compiled"/>
        <FIELD NAME="compiled" TYPE="enum('0', '1')" NOTNULL="false" DEFAULT="NULL" PREVIOUS="finish" NEXT="taken"/>
        <FIELD NAME="taken" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="NULL" PREVIOUS="id" NEXT="passed"/>
        <FIELD NAME="passed" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="NULL" PREVIOUS="taken"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

  </TABLES>
</XMLDB>

